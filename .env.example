# JAMA Connect OAuth Configuration
# Copy this file to .env and fill in your credentials

# JAMA Connect Instance URL (without trailing slash)
JAMA_BASE_URL=https://jama.iscinternal.com

# OAuth 2.0 Client Credentials
# Obtain from JAMA Admin -> Organization Settings -> OAuth Clients
JAMA_CLIENT_ID=your_client_id_here
JAMA_CLIENT_SECRET=your_client_secret_here

# Optional: Enable mock mode for testing without credentials
# JAMA_MOCK_MODE=false

# Optional: Set log level (DEBUG, INFO, WARNING, ERROR)
# LOG_LEVEL=INFO
